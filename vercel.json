{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=60" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/dashboard.html",
      "permanent": true
    }
  ]
}
